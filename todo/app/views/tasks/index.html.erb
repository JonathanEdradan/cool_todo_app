<h1>Tasks#index</h1>
<ul>
	<% @tasks.each do |task| %>
	<li>
		<div>Content: <%= task.content %> </div>
		<div>Complete: <%= task.complete %> </div>
		<div>Created At: <%= task.created_at %> </div>
		<div>Id: <%= task.id %> </div>
		<%= button_to "Delete", task, method: :delete %>
	</li>
	<% end %>
</ul>