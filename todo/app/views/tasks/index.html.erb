<h1>Your Tasks</h1>
<ul>
	<% @tasks.each do |task| %>
	<li>
		<div>Content: <%= task.content %> </div>
		<div>Complete: <%= task.complete %> </div>
		<div>Created At: <%= task.created_at %> </div>
		<div>Id: <%= task.id %> </div>
		<%= button_to "Delete", destroy_task_path(@user,task), method: :delete %>
	</li>
	<% end %>
</ul>